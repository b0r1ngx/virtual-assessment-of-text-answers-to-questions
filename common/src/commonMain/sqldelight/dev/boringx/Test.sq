CREATE TABLE Test(
  id          INTEGER PRIMARY KEY,
  creator_id  INTEGER,
  course_id   INTEGER,
  name        TEXT,
--   start_at    DATE,
--   end_at      DATE,
--   created_at  DATE

  FOREIGN KEY(creator_id) REFERENCES User(id),
  FOREIGN KEY(course_id) REFERENCES Course(id)
);

getTests:
SELECT * FROM Test
WHERE course_id = ?;

createTest:
INSERT INTO Test(creator_id, course_id, name)
VALUES(?, ?, ?);
