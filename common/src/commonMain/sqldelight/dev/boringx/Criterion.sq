INSERT INTO Criterion(name) VALUES ('Completeness');
INSERT INTO Criterion(name) VALUES ('Coherence');

selectAll:
SELECT * FROM Criterion;

selectCriterionName:
SELECT name FROM Criterion
WHERE id = ?;
